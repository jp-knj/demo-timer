{
    "include": [
        "client"
    ],
    "compilerOptions": {
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true
    }
}
