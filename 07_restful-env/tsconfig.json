{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "public",
    "baseUrl": "server",
    "lib": [
      "es2015"
    ],
    "paths": {
      "*": ["node_modules/*","server/types/*"]
    }
  },
  "include": [
    "server/**/*"
  ]
}
